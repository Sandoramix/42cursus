{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true
	},
	"files.associations": {
		"*.html": "html",
		".env": "dotenv",
		"string": "cpp",
		"iostream": "cpp",
		"*.tcc": "cpp",
		"iomanip": "cpp"
	},
	"C_Cpp.default.cStandard": "c99",
	"C_Cpp.default.cppStandard": "c++98"
}